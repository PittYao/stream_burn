                },
                "rtspUrlRight": {
                    "type": "string",
                    "example": "rtsp://admin:cebon61332433@192.168.99.215:554/cam/realmonitor?channel=1\u0026subtype=1"
                },
                "temperature": {
                    "type": "string",
                    "example": ""
                }
            }
        },
